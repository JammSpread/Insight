{
	"author": {
		"name": "JK",
		"email": "u.sslooky@gmail.com"
	},
	"name": "intrasight",
	"displayName": "Intrasight",
	"description": "A fork of @TylerMcGinn's VS Code extension that allows you to search external sites within the editor.",
	"version": "1.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/JammSpread/Intrasight.git"
	},
	"icon": "media/icon.png",
	"publisher": "undefined",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"vscode",
		"extension",
		"search"
	],
	"activationEvents": [
		"onStartupFinished",
		"onView:Resources.customSearch",
		"onView:Resources.search",
		"onView:Docs.launch",
		"onView:StackOverflow.search",
		"onView:StackOverflow.launch"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "Resources.search",
				"title": "Search"
			},
			{
				"command": "Resources.customSearch",
				"title": "Custom Search",
				"icon": {
					"dark": "media/dark/search.png",
					"light": "media/light/search.png"
				}
			},
			{
				"command": "Docs.launch",
				"title": "Launch Document in Browser"
			},
			{
				"command": "StackOverflow.search",
				"title": "Search StackOverflow",
				"icon": {
					"dark": "media/dark/search.png",
					"light": "media/light/search.png"
				}
			},
			{
				"command": "StackOverflow.launch",
				"title": "Launch StackOverFlow in Browser"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "Menu",
					"title": "Intrasight",
					"icon": "media/menu.svg"
				}
			]
		},
		"views": {
			"Menu": [
				{
					"id": "Menu1",
					"name": "Search Resources"
				},
				{
					"id": "Menu2",
					"name": "StackOverflow Search"
				},
				{
					"id": "Menu3",
					"name": "Docs"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "StackOverflow.search",
					"when": "view == Menu2",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "Resources.customSearch",
					"when": "view == Menu1 && viewItem == MenuItem",
					"group": "inline"
				}
			]
		},
		"configuration": {
			"title": "Intrasight Extension Configuration",
			"properties": {
				"Intrasight.StackOverflowNumberOfDisplayedResults": {
					"type": "number",
					"default": 15,
					"description": "The number of results to be displayed from a search of StackOverflow. Default 15.",
					"scope": "resource"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn format && yarn compile",
		"vscode:publish": "vsce package && date",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn compile && node ./node_modules/vscode/bin/test",
		"format": "./format.sh"
	},
	"dependencies": {
		"axios": "^0.21.1"
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"prettier": "^2.2.1",
		"vscode": "^1.1.28"
	}
}
