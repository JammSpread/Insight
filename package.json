{
	"author": {
		"name": "Tyler McGinn",
		"email": "systematyler@gmail.com"
	},
	"name": "insight",
	"displayName": "Insight",
	"description": "VS Code extension for quick access and smart queries of learning resources such as Google,Stackoverflow,Youtube and W3 Schools.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onView:Resources.customSearch",
		"onView:Resources.search",
		"onView:Docs.launch",
		"onView:StackOverflow.search",
		"onView:StackOverflow.launch",
		"onCommand:stack"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "Resources.search",
				"title": "Search"
			},
			{
				"command": "Resources.customSearch",
				"title": "Custom Search",
				"icon": {
					"dark": "Icons/Dark/search.png",
					"light": "Icons/Light/search.png"
				}
			},
			{
				"command": "Docs.launch",
				"title": "Launch Document in Browser"
			},
			{
				"command": "stack",
				"title": "stack open"
			},
			{
				"command": "StackOverflow.seach",
				"title": "Seach StackOverflow",
				"icon":{
					"dark": "Icons/Dark/search.png",
					"light": "Icons/Light/search.png"
				}
			},
			{
				"command": "StackOverflow.launch",
				"title": "Launch StackOverFlow in Browser"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "Menu",
					"title": "Insight",
					"icon": "Icons/darkeye.png"
				}
			]
		},
		"views": {
			"Menu": [
				{
					"id": "Menu1",
					"name": "Search Resources"
				},
				{
					"id": "Menu2",
					"name": "Docs"
				},
				{
					"id": "Menu3",
					"name": "StackOverflow Search"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "StackOverflow.seach",
					"when": "view == Menu3",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "Resources.customSearch",
					"when": "view == Menu1 && viewItem == MenuItem",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
